{
  "name": "@opensdks/sdk-openai",
  "version": "0.0.1",
  "module": "./index.ts",
  "files": [
    "dist",
    "**/*.ts",
    "!**/*.spec.ts"
  ],
  "scripts": {
    "build": "concurrently npm:build:*",
    "build:dts": "cp ./openai.oas.ts ./dist/openai.oas.ts",
    "build:json": "npx tsx ../../bin/jsonToJs.ts ./dist/openai.oas.json",
    "build:ts": "tsc -p ./tsconfig.json",
    "clean": "rm -rf ./dist",
    "download": "curl --fail https://raw.githubusercontent.com/openai/openai-openapi/master/openapi.yaml -o ./openai.oas.yaml",
    "generate": "cat openai.oas.yaml | node ../../bin/yaml2json.js -o openai.oas && openapi-typescript ./openai.oas.json --output ./openai.oas.ts",
    "pub": "pnpm publish --no-git-checks --access public"
  },
  "dependencies": {},
  "devDependencies": {
    "@opensdks/runtime": "workspace:*",
    "concurrently": "^8.2.2",
    "openapi-typescript": "6.7.1",
    "yaml": "2.3.4"
  },
  "publishConfig": {
    "access": "public"
  }
}
