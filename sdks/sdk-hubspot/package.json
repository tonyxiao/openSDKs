{
  "name": "@opensdks/sdk-hubspot",
  "version": "0.0.4",
  "type": "module",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./esm/index.js",
      "require": "./cjs/index.js"
    },
    "./*.oas.types": "./*.oas.types.js",
    "./*.oas.json": "./*.oas.json",
    "./*": {
      "types": "./types/*.d.ts",
      "import": "./esm/*.js",
      "require": "./cjs/*.js"
    }
  },
  "main": "./cjs/index.js",
  "module": "./esm/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "types",
    "esm",
    "cjs",
    "src",
    "*.ts",
    "*.d.ts",
    "*.oas.json",
    "!**/*.spec.ts",
    "!*.orig.oas.json"
  ],
  "scripts": {
    "build": "concurrently npm:build:*",
    "build:cjs": "tsc -p ./tsconfig.build.json --declaration false --declarationMap false --module CommonJS --moduleResolution Node10 --outDir ./cjs",
    "build:cjs-pkgjson": "mkdir -p ./cjs && echo '{\"type\": \"commonjs\"}' > ./cjs/package.json",
    "build:esm": "tsc -p ./tsconfig.build.json --declaration false --declarationMap false --outDir ./esm",
    "build:types": "tsc -p ./tsconfig.build.json --emitDeclarationOnly --outDir ./types",
    "clean": "rm -rf ./esm ./cjs ./types",
    "debug": "npx tsx ../../bin/jsonToJs.ts dist/",
    "download": "concurrently npm:download:*",
    "download:crm-associations": "curl --fail https://api.hubspot.com/api-catalog-public/v1/apis/crm/v4/associations -o ./hubspot_crm_associations.oas.json",
    "download:crm-companies": "curl --fail https://api.hubspot.com/api-catalog-public/v1/apis/crm/v3/objects/companies -o ./hubspot_crm_companies.oas.json",
    "download:crm-contacts": "curl --fail https://api.hubspot.com/api-catalog-public/v1/apis/crm/v3/objects/contacts -o ./hubspot_crm_contacts.oas.json",
    "download:crm-deals": "curl --fail https://api.hubspot.com/api-catalog-public/v1/apis/crm/v3/objects/deals -o ./hubspot_crm_deals.oas.json",
    "generate": "concurrently npm:generate:*",
    "generate:crm-associations": "node ../../packages/cli/generateFromOas.js ./hubspot_crm_associations.oas.json",
    "generate:crm-companies": "node ../../packages/cli/generateFromOas.js ./hubspot_crm_companies.oas.json",
    "generate:crm-contacts": "node ../../packages/cli/generateFromOas.js ./hubspot_crm_contacts.oas.json",
    "generate:crm-deals": "node ../../packages/cli/generateFromOas.js ./hubspot_crm_deals.oas.json",
    "version": "pnpm version"
  },
  "dependencies": {},
  "devDependencies": {
    "@opensdks/runtime": "workspace:*",
    "concurrently": "^8.2.2",
    "openapi-typescript": "6.7.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
