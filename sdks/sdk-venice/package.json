{
  "name": "@opensdks/sdk-venice",
  "version": "0.0.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "**/*.ts",
    "!**/*.spec.ts",
    "!**/*.json"
  ],
  "scripts": {
    "build": "concurrently npm:build:*",
    "build:dts": "mkdir -p dist && cp *.d.ts ./dist",
    "build:json": "mkdir -p dist && npx tsx ../../bin/oasJsonToJs.ts ./ ./dist",
    "build:ts": "tsc -p ./tsconfig.build.json",
    "clean": "rm -rf ./dist",
    "download": "curl --fail https://app.venice.is/api/v0/openapi.json | prettier --parser json > ./venice.oas.json",
    "generate": "openapi-typescript ./venice.oas.json --output venice.oas.d.ts"
  },
  "dependencies": {},
  "devDependencies": {
    "@opensdks/runtime": "workspace:*",
    "concurrently": "^8.2.2",
    "openapi-typescript": "6.7.1",
    "prettier": "3.1.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "exports": {
    ".": "./dist/index.js",
    "./*": "./dist/*"
  }
}
